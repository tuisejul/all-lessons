def print_params(a=1, b='строка',c=True):
    print(a,b,c)

print_params()
print_params(4,'bub', False)
print_params('dd',5,3) #Задача1

def print_params1(a=1, b='строка',c=True):
    print_params(a, b, c)

value_list = ['girl', 'boy', 'cat']
values_dict = {'a':'girl', 'b':'boy', 'c':'cat'}
values_list_2 = [54.32, 'Строка' ]
print_params1(*value_list)
print_params1(**values_dict)
print_params1(*values_list_2, 42)    #Задача2-3
